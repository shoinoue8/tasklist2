<h1><%= current_user.name %>さんのタスク一覧</h1>
<ul>
  <% @tasks.each do |task| %>
  <li><%= link_to task.id,task %> : <%= task.content %> &gt; <%= task.status %></li>
  <% end %>
</ul>

<%= link_to '新規タスクの登録', new_task_path %>
<%= link_to 'ログアウト', logout_path, method: :delete, data: { confirm: 'ログアウトしてもよろしいですか？'} %>